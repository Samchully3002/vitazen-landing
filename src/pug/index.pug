doctype html
html(lang='en')

    include includes/head.pug
    body
        include includes/header.pug
        // Section hero banner 
        section.hero
            .carousel.slide#heroCarousel(data-bs-ride="carousel")
                .carousel-indicators
                    button(type="button", data-bs-target="#heroCarousel", data-bs-slide-to="0", class="active", aria-current="true", aria-label="Slide 1")
                    button(type="button", data-bs-target="#heroCarousel", data-bs-slide-to="1", aria-label="Slide 2")
                    button(type="button", data-bs-target="#heroCarousel", data-bs-slide-to="2", aria-label="Slide 3")
                .carousel-inner
                    .carousel-item.active
                        .row.align-items-center
                            img(src="./assets/img/img_hero1.png", alt="Snovv Banner")
                    .carousel-item
                        .row.align-items-center
                            img(src="./assets/img/img_hero1.png", alt="Snovv Banner")
                    .carousel-item
                        .row.align-items-center
                            img(src="./assets/img/img_hero1.png", alt="Snovv Banner")

        // Section Promotion Youtube Video
        section.vidYoutube
            .container.text-center
                //- iframe.vid(src="https://www.youtube.com/embed/eVlV9dN4nn4")
                iframe.vid(src="https://www.youtube.com/embed/eVlV9dN4nn4" title="SNOVV - Lebih Dari Sekedar Minuman" frameborder="0" allow="accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture; web-share" referrerpolicy="strict-origin-when-cross-origin" allowfullscreen)

        // Section Top Product
        section.products.py-5
            .product_container
                .wrapper
                    h1 Products
                    a.btn view more
                .row
                    //- Product 1
                    .col-md-4.mb-4
                        a(href="productDetail.html")
                            .card.border-0.shadow
                                img.card-img-top(src="./assets/img/product1.png", alt="Snovv Glutathione")
                                .card-body.text-center
                                    h5.card-title Snovv Glutathione
                                    p.card-price
                                        span.price-new Rp250.000
                                        span.price-old.text-muted Rp350.000
                    .col-md-4.mb-4
                        .card.border-0.shadow
                            img.card-img-top(src="./assets/img/product2.png", alt="Snovv Magnesium")
                            .card-body.text-center
                                h5.card-title Snovv Magnesium
                                p.card-price
                                    span.price-new Rp150.000
                                    span.price-old.text-muted Rp300.000
                    .col-md-4.mb-4
                        .card.border-0.shadow
                            img.card-img-top(src="./assets/img/product3.png", alt="Snovv VitC")
                            .card-body.text-center
                                h5.card-title Snovv Liposome Vitamin C
                                p.card-price
                                    span.price-new Rp150.000
                                    span.price-old.text-muted Rp300.000

